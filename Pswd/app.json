    },
    "android": {
      "package": "com.cristian.pswd",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b0350db1-979f-455d-8200-d62f7192fe45"
      }
    }
  }
}
